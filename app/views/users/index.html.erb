<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        </span>
        <% if current_user.id != user.id %>
          <span class="friend_button"></span>
        <% end %>
      </li>
    <% end %>
  </ul>
  </ul>
</section>
